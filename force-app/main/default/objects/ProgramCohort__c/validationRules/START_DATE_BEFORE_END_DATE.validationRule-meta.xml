<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>START_DATE_BEFORE_END_DATE</fullName>
    <active>true</active>
    <description>Ensures the start date is before the end date.</description>
    <errorConditionFormula>AND(
  NOT(ISBLANK(StartDate__c)),
  NOT(ISBLANK(EndDate__c)),
  EndDate__c &lt; StartDate__c
)</errorConditionFormula>
    <errorMessage>Start Date must be before End Date</errorMessage>
</ValidationRule>
